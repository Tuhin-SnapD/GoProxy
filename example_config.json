{
  "server": {
    "port": 8080,
    "read_timeout": "30s",
    "write_timeout": "30s",
    "idle_timeout": "60s"
  },
  "backend": {
    "url": "http://localhost:8081",
    "health_check_path": "/health",
    "health_check_interval": "30s"
  },
  "cache": {
    "ttl": "5m",
    "max_size": 1000,
    "cleanup_interval": "1m"
  },
  "rate_limit": {
    "requests_per_minute": 100,
    "burst_size": 10,
    "cleanup_interval": "5m"
  },
  "metrics": {
    "enabled": true,
    "path": "/metrics",
    "retention_period": "24h"
  },
  "logging": {
    "level": "info",
    "format": "json",
    "output": "stdout"
  }
} 